package K6_컨트롤러심화_프로젝트1_멤버;

public class Member {
	private String id;
	private String pw;
	private String name;
	
	
	
	public String getId() {
		return id;
	}



	public void setId(String id) {
		this.id = id;
	}



	public String getPw() {
		return pw;
	}



	public void setPw(String pw) {
		this.pw = pw;
	}



	public String getName() {
		return name;
	}



	public void setName(String name) {
		this.name = name;
	}



	void printMember() {
		System.out.println(id + " : " + pw + " : " + name);
	}
}
